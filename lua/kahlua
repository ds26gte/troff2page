# last modified 2017-08-20

rm -f ../troff2page.lua *-Z-*.lua

for f in *.lua; do
  cat $f |grep -v '^-- last modified' >> ../troff2page.lua
  echo >> ../troff2page.lua
done

DEBUG=false

echo "troff2page(Troff2page_file_arg, $DEBUG)" >> ../troff2page.lua 

chmod +x ../troff2page.lua

nvim -es ../troff2page.lua -cwq
