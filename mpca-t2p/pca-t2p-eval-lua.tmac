defrequest('pca-eval:process-snippets', function()
  read_troff_line()
  local f = String_table['pca-eval:file']()
  if probe_file(f) then dofile(f) end
end)

defrequest('pca-eval:load-snippet-if-poss', function()
  read_troff_line()
  local f = String_table['pca-eval:snippet-file']()
  if probe_file(f) then troff2page_file(f)
  else flag_missing_piece('eval')
  end
end)
