.if !\n[.troff2page] .nx
.
.\" these macros are relevant only for troff2page,
.\" which uses them to create and insert a table
.\" of contents for man pages.
.\" Place this file in GROFF_TMAC_PATH, cwd, or home
.
.if d process_toc_line .nx
.
.mso pca-toc.tmac
.
.rn TH TH_orig
.
.de TH
.TH_orig \\$*
.TOC
..
.
.rn SH SH_orig
.
.de SH
.process_toc_line 1 \\$*
.SH_orig \\$*
..
.
.rn SS SS_orig
.
.de SS
.process_toc_line 2 \\$*
.SS_orig \\$*
..
